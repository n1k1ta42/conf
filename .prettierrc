{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": false,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/pages/(.*)$",
    "^@/domain/(.*)$",
    "^@/application/(.*)$",
    "^@/services/(.*)$",
    "^@/components/(.*)$",
    "^@/store/(.*)$",
    "^@/styles/(.*)$",
    "^@/utils/(.*)$",
    "^@/constants/(.*)$",
    "^../(.*)$",
    "^./(.*)$",
    "^./(.css*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
