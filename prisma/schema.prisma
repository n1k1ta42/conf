// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  name          String
  surname       String
  email         String
  phone         String
  isFamiliar    Boolean
  role          String?
  position      String?
  company       String?
  stack         String?
  sphere        String?
}
